license_create_manifest() {
        # Test if BUILD_IMAGES_FROM_FEEDS is defined in env
        if [ -n "${BUILD_IMAGES_FROM_FEEDS}" ]; then
          exit 0
        fi

	INSTALLED_PKGS=`cat /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/deploy/licenses/core-image-wvsnp-minimal-wandboard-dual-20160922003828/package.manifest`
	LICENSE_MANIFEST="/home/carl/wvsnp_image/fsl-community-bsp/build/tmp/deploy/licenses/core-image-wvsnp-minimal-wandboard-dual-20160922003828/license.manifest"
	# remove existing license.manifest file
	if [ -f ${LICENSE_MANIFEST} ]; then
		rm ${LICENSE_MANIFEST}
	fi
	touch ${LICENSE_MANIFEST}
	for pkg in ${INSTALLED_PKGS}; do
		filename=`ls /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/sysroots/wandboard-dual/pkgdata/runtime-reverse/${pkg}| head -1`
		pkged_pn="$(sed -n 's/^PN: //p' ${filename})"

		# check to see if the package name exists in the manifest. if so, bail.
		if grep -q "^PACKAGE NAME: ${pkg}" ${LICENSE_MANIFEST}; then
			continue
		fi

		pkged_pv="$(sed -n 's/^PV: //p' ${filename})"
		pkged_name="$(basename $(readlink ${filename}))"
		pkged_lic="$(sed -n "/^LICENSE_${pkged_name}: /{ s/^LICENSE_${pkged_name}: //; p }" ${filename})"
		pkged_size="$(sed -n "/^PKGSIZE_${pkged_name}: /{ s/^PKGSIZE_${pkged_name}: //; p }" ${filename})"
		if [ -z "${pkged_lic}" ]; then
			# fallback checking value of LICENSE
			pkged_lic="$(sed -n "/^LICENSE: /{ s/^LICENSE: //; p }" ${filename})"
		fi

		echo "PACKAGE NAME:" ${pkg} >> ${LICENSE_MANIFEST}
		echo "PACKAGE VERSION:" ${pkged_pv} >> ${LICENSE_MANIFEST}
		echo "RECIPE NAME:" ${pkged_pn} >> ${LICENSE_MANIFEST}
		echo "LICENSE:" ${pkged_lic} >> ${LICENSE_MANIFEST}
		echo "" >> ${LICENSE_MANIFEST}

		# If the package doesn't contain any file, that is, its size is 0, the license
		# isn't relevant as far as the final image is concerned. So doing license check
		# doesn't make much sense, skip it.
		if [ "$pkged_size" = "0" ]; then
			continue
		fi

		lics="$(echo ${pkged_lic} | sed "s/[|&()*]/ /g" | sed "s/  */ /g" )"
		for lic in ${lics}; do
			# add explicity avoid of CLOSED license because isn't generic
			if [ "$lic" = "CLOSED" ]; then
				continue
			fi

			# remove + chars in any position this cover cases like
			# GPL-2.0+-with-OpenSSL-exception -> GPL-2.0-with-OpenSSL-exception
			lic="$(echo ${lic} | sed "s/\+//g")"

			# to reference a license file trim trailing + symbol
			if ! [ -e "/home/carl/wvsnp_image/fsl-community-bsp/build/tmp/deploy/licenses/${pkged_pn}/generic_${lic}" ]; then
				bbwarn "The license listed ${lic} was not in the licenses collected for ${pkged_pn}"
			fi
		done
	done

	# Two options here:
	# - Just copy the manifest
	# - Copy the manifest and the license directories
	# With both options set we see a .5 M increase in core-image-minimal
	if [ "${COPY_LIC_MANIFEST}" = "1" ]; then
		mkdir -p /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/work/wandboard_dual-poky-linux-gnueabi/core-image-wvsnp-minimal/1.0-r0/rootfs/usr/share/common-licenses/
		cp ${LICENSE_MANIFEST} /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/work/wandboard_dual-poky-linux-gnueabi/core-image-wvsnp-minimal/1.0-r0/rootfs/usr/share/common-licenses/license.manifest
		if [ "${COPY_LIC_DIRS}" = "1" ]; then
			for pkg in ${INSTALLED_PKGS}; do
				mkdir -p /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/work/wandboard_dual-poky-linux-gnueabi/core-image-wvsnp-minimal/1.0-r0/rootfs/usr/share/common-licenses/${pkg}
				pkged_pn="$(oe-pkgdata-util -p /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/sysroots/wandboard-dual/pkgdata lookup-recipe ${pkg})"
				for lic in `ls /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/deploy/licenses/${pkged_pn}`; do
					# Really don't need to copy the generics as they're
					# represented in the manifest and in the actual pkg licenses
					# Doing so would make your image quite a bit larger
					if [ "${lic#generic_}" = "${lic}" ]; then
						cp /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/deploy/licenses/${pkged_pn}/${lic} /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/work/wandboard_dual-poky-linux-gnueabi/core-image-wvsnp-minimal/1.0-r0/rootfs/usr/share/common-licenses/${pkg}/${lic}
					else
						if [ ! -f /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/work/wandboard_dual-poky-linux-gnueabi/core-image-wvsnp-minimal/1.0-r0/rootfs/usr/share/common-licenses/${lic} ]; then
							cp /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/deploy/licenses/${pkged_pn}/${lic} /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/work/wandboard_dual-poky-linux-gnueabi/core-image-wvsnp-minimal/1.0-r0/rootfs/usr/share/common-licenses/
						fi
						ln -sf ../${lic} /home/carl/wvsnp_image/fsl-community-bsp/build/tmp/work/wandboard_dual-poky-linux-gnueabi/core-image-wvsnp-minimal/1.0-r0/rootfs/usr/share/common-licenses/${pkg}/${lic}
					fi
				done
			done
		fi
	fi


}

bbwarn() {
	echo "WARNING: $*"

}

cd '/home/carl/wvsnp_image/fsl-community-bsp/build/tmp/work/wandboard_dual-poky-linux-gnueabi/core-image-wvsnp-minimal/1.0-r0/core-image-wvsnp-minimal-1.0'
license_create_manifest

# cleanup
ret=$?
